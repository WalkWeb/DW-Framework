{
  "name": "walkweb/dw-framework",
  "description": "Micro MVC Framework",
  "authors": [
    {
      "name": "WalkWeb",
      "email": "av18775@list.ru"
    }
  ],
  "config": {
    "soft-packages": true
  },
  "require": {
    "php": ">=7.4.0",
    "ext-mbstring": "*",
    "ext-gd": "*",
    "ext-mysqli": "*",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "ramsey/uuid": "^4.2",
    "phpmailer/phpmailer": "^6.9"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Handler\\": "Handler/",
      "Middleware\\": "Middleware/",
      "Repository\\": "Repository/",
      "Domain\\": "Domain/",
      "NW\\": "src/NWFramework",
      "Migrations\\": "migrations/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
